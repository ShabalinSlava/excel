{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "rules": {
    "semi": "off",
    "linebreak-style": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "indent": ["error", 2, {"SwitchCase": 1}],
    "quotes": [
      "error",
      "single"
    ]
  },
  "plugins": ["jest"],
  "extends": ["eslint:recommended", "google"]
}